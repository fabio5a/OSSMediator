    {
      "api": "/network-hardware-groups/{nhg_id}/fmdata",
      "type": "HISTORY",
      "metric_type": "RADIO",
      "interval": 1,
      "sync_duration": 15
    },
    {
      "api": "/network-hardware-groups/{nhg_id}/fmdata",
      "type": "ACTIVE",
      "metric_type": "DAC",
      "interval": 1,
      "sync_duration": 15
    },
    {
      "api": "/network-hardware-groups/{nhg_id}/fmdata",
      "type": "HISTORY",
      "metric_type": "DAC",
      "interval": 1,
      "sync_duration": 15
